    "e2e": "node test/e2e/runner.js",
    "test1": "jest",
    "test": "ava ./test/test-vue.js",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "deploy": "rsync -avzh --progress ./dist/. vtolbert@victortolbert.com:/home/vtolbert/demo.videa.design"
  },
